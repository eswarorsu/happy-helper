# =============================================================================
# INNOVESTOR â€“ Frontend Environment Variables
# Copy this file to .env and fill in your actual values.
# NEVER commit .env to git.
# =============================================================================

# --- Supabase ---
# https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=YOUR_SUPABASE_ANON_KEY

# --- Firebase (client-side) ---
# https://console.firebase.google.com/project/YOUR_PROJECT/settings/general
# These are PUBLIC identifiers, not secrets, but keep them out of source control
# so you can rotate or swap environments without changing code.
VITE_FIREBASE_API_KEY=YOUR_FIREBASE_API_KEY
VITE_FIREBASE_AUTH_DOMAIN=YOUR_PROJECT.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://YOUR_PROJECT-default-rtdb.REGION.firebasedatabase.app
VITE_FIREBASE_PROJECT_ID=YOUR_PROJECT
VITE_FIREBASE_STORAGE_BUCKET=YOUR_PROJECT.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=YOUR_SENDER_ID
VITE_FIREBASE_APP_ID=YOUR_APP_ID

# --- Backend API ---
# URL of the Express backend (local dev or deployed on Render / Railway)
VITE_BACKEND_URL=http://localhost:5050
